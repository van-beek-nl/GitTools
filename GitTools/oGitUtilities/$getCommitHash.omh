If not($cinst.$$execute(con('log -1 --pretty=format:"%H" -- "',pPath,'"'),lvOutput))
	# Hotfix: git log returns an error when the repository exists but does not have any commits yet.
	# For now, return an empty string when this occurs.
	If pos('does not have any commits yet',lvOutput)>0
		Quit method ""
	End If
	
	Signal error -1,lvOutput
End If

Quit method lvOutput