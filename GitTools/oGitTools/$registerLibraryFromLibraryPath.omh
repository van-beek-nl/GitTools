Do tvFileUtilities.$libraryPathToConfigPath(pLibraryPath) Returns lvConfigPath
If not(FileOps.$doesfileexist(lvConfigPath))&not(tvSettings.$allowRegistrationWithoutConfigFile)
	Do lvFileOps.$createfile(lvConfigPath)
	Do lvFileOps.$writecharacter(kUniTypeUTF8,"{}")
	Do lvFileOps.$closefile()
End If

Quit method $cinst.$registerLibraryFromConfigPath(lvConfigPath)